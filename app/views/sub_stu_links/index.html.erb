<p id="notice"><%= notice %></p>

<h1>Sub Stu Links</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_stu_links.each do |sub_stu_link| %>
      <tr>
        <td><%= link_to 'Destroy', sub_stu_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub Stu Link', new_sub_stu_link_path %>
