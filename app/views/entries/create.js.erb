<!-- replace partial with blank -->
$('#task<%= @task.id %>')
    .replaceWith("<%= escape_javascript render(partial: 'plans/entry', task: @task) %>");

<!-- set partial contents to entry contents -->

 $('#task<%= @task.id %>').find('.editable').prop('value', '<%= Entry.where({task: @task, student: @entry.student}).last.student_response %>');

<!-- disable field -->
  $('#task<%= @task.id %>').find('.editable').prop('disabled', 'disabled');



<!-- Take @child_tasks of @task and find @next_task   -->

<% if @next_task %>

  $('#task<%= @task.id%>').after("<%= escape_javascript render(partial: 'plans/entry', task: @next_task) %>"); //Doesn't produce correct task..?

<% end %>